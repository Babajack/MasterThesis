{
	"compilerOptions": {
		"target": "ESNext",
		"module": "CommonJS",
		"lib": ["es6"],
		"allowJs": true,
		"jsx": "react",
		//"outDir": "build",
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"baseUrl": "./src",
		"moduleResolution": "node",
		"outDir": "./build"
	}
}
